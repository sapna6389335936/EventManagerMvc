@inject SignInManager<AppUser> signInManager;
@model EventManager.Models.ViewModels.Login

@{
    ViewData["Title"] = "Login";
    Layout = null;// "~/Views/Shared/_LandingPageLayout.cshtml";
}
<!DOCTYPE html>
<html lang="en">

